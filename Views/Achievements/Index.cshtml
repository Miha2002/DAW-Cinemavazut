@model IEnumerable<Proiect_DAW_Cinemavazut.Models.Achievements>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.denumire)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.categorie_ach)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.prag)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.denumire)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.categorie_ach)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.prag)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_achievement">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_achievement">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_achievement">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
